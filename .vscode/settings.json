{
	"files.exclude": {
		"node_modules": true,
		"Build": true,
		"package-lock.json": true,
		".gitignore": true
	},
	"eslint.run": "onType",
	"eslint.workingDirectories": ["Script"],
	"eslint.validate": [
		"typescript",
		"typescriptreact"
	],
	"eslint.enable": true,
	"files.eol": "\n",
	"files.encoding": "utf8",
	"files.insertFinalNewline": true,
	"editor.insertSpaces": false,
	"editor.detectIndentation": true,
	"files.trimTrailingWhitespace": true
}
